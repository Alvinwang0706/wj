<!DOCTYPE html>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8" import="com.sinosoft.framework.*"%>
<%@ taglib uri="/struts-tags" prefix="s"%>
<%
	String serverContext = Config.getServerContext();
%>
<html> 
<head> 
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>会员中心-常用信息</title>
<!-- 会员中心公共CSS -->
<s:include value="/wwwroot/kxb/block/kxb_member_center_css.shtml"></s:include>
<link rel="stylesheet" type="text/css" href="<%=Config.getValue("StaticResourcePath")%>/style/skins/default.css"/>
<script type="text/javascript">
</script> 
<s:include value="/wwwroot/kxb/block/kxb_custom_header.shtml"></s:include>
</head>
<body class="up-bg-qh">
		<!-- 顶部 开始 1-->
		<s:include value="/wwwroot/kxb/block/kxb_header_index_new_v2.shtml"></s:include>
		<!-- 顶部 结束 -->
	<div class="wrapper">
		<div class="daohang">
			<span class="daohang_home"></span>
			<a href='<%=Config.getFrontServerContextPath() %>' target='_self' ><span class="orange">您现在的位置：首页</span></a><span class="separator">></span><a href='member_center!index.action' target='_self'>会员中心</a><span class="separator">></span><span class="separator1">常用信息</span>
		</div>
		<div class="member_con">
		    <jsp:include page="member_center_left.jsp"></jsp:include>
		 	<input type="hidden" value='<s:property value="#request.deliverAddressCount" />' id="deliverAddrCount" name="deliverAddrCount">
		 	<div class="member_right mbr_bg">
		 		<!-- 常用信息维护 -->
		 		<div class="member_boxs">
		 			<div class="member-htit clear-bor-b"><span class="member-titsc">常用投保人信息维护</span></div>
		 	     	
		 	     	<div class="management_con cf"> 
			 	     	<div class="add_management cf">
							<span class="add_manage" id="artAppntInsert"></span>
							<div class="manage_mes">已有<b><s:property value="#request.appntCount"/></b>个常用投保人信息，还可添加<b><s:property value="#request.leftAppntCount"/></b>个</div>
						</div>
						<div id="appnt">
							<s:iterator id="list" value="#request.reappntList" step="1" status="st">
								<div class="managements_con">
									<table width="100%" border="0">
										<tr>
											<td>
												<label>姓名：</label>
												<span><s:property value="#list.applicantName" /></span>
												<span style="display: none"><s:property value="#list.id" /></span>
											</td>
											<td>
							                    <label>英文名：</label>
							                    <span><s:property value="#list.applicantEnName" /></span>
							                </td>
										</tr>
										<tr>
											<td>
												<label>性别：</label>
												<span><s:property value="#list.applicantSexName" /></span>
											</td>
											<td>
												<label>证件类型：</label>
												<span><s:property value="#list.applicantIdentityTypeName" />&nbsp;&nbsp;<s:property value="#list.applicantIdentityId" /></span>
											</td>
										</tr>
										<tr>
											<td>
												<label>手机号码：</label>
												<span><s:property value="#list.applicantMobile" /></span>
											</td>
											<td>
							                    <label>出生日期：</label>
							                    <span><s:property value="#list.applicantBirthday" /></span>
							                </td>
										</tr>
										<tr>
											<td>
							                    <label>电子邮箱：</label>
							                    <span><s:property value="#list.applicantMail" /></span>
							                </td>
							                <td></td>
										</tr>
									</table>
									<div class="manage_make">
										<span class="manage_update_appnt" id="artAppntUpdate_<s:property value="#list.id" />">修改</span>
										<span class="manage_del"onclick="deleteinfo('appnt','<s:property value="#list.id" />');">删除</span>
									</div>
								</div>
							</s:iterator>
							<p class="art_insert" onclick="jQuery('#artAppntInsert').click()"><span class="icon_a"></span>添加常用投保人</p> 
						</div>
		 	  	   </div>
		 	  	   <div class="member-htit clear-bor-b"><span class="member-titsc">常用被保人信息维护</span></div>
		 	  	   <div class="cz_membertb">
		 	  	   	   <div id="insured">
		 	  	           <div class="management_con cf"> 
		 	  	        	   <div class="add_management"><span class="add_manage" id="artRecognizeeInsert"></span><div class="manage_mes">已有<b> <s:property value="#request.insuredCount"/></b>个常用被保人信息，还可添加<b><s:property value="#request.leftInsuredCount"/></b>个</div></div>
	                       		<s:iterator id="list" value="#request.recognizeeList" step="1" status="st">
		                       		<div class="managements_con"> 
		                       			<table width="100%" border="0">
			                       			<tr>
												<td>
													<label>姓名：</label>
													<span><s:property value="#list.recognizeeName" /></span>
													<span style="display: none"><s:property value="#list.id" /></span>
												</td>
							                    <td>
									                <label>英文名：</label>
									                <span><s:property value="#list.recognizeeEnName" /></span>
									            </td>
											</tr>
											<tr>
												<td>
													<label>性别：</label>
													<span><s:property value="#list.RecognizeeSexName" /></span>
												</td>
												<td>
													<label>证件类型：</label>
													<span><s:property value="#list.RecognizeeIdentityTypeName" /> &nbsp;&nbsp;<s:property value="#list.RecognizeeIdentityId" /></span>
												</td>
											</tr>
											<tr>
												<td>
													<label>手机号码：</label>
													<span><s:property value="#list.RecognizeeMobile" /></span>
												</td>
												<td>
								                    <label>出生日期：</label>
								                    <span><s:property value="#list.RecognizeeBirthday" /></span>
								                </td>
											</tr>
											<tr>
								                <td>
								                    <label>电子邮箱：</label>
								                    <span><s:property value="#list.RecognizeeMail" /></span>
								                </td>
								                <td>
								                </td>
								            </tr>
		                       			</table>
		                       			<div class="manage_make">
		                       				<span class="manage_update_insured" id="artInsuredUpdate_<s:property value="#list.id" />">修改</span>
		                       				<span class="manage_del" onclick="deleteinfo('insured','<s:property value="#list.id" />');">删除</span>
		                       			</div>
									</div>
			                	</s:iterator>
			                	<p class="art_insert" onclick="jQuery('#artRecognizeeInsert').click()"><span class="icon_a"></span>添加常用被保人</p>
		 	  	   			</div>   
		 				</div>
		 			</div>
		 			<div class="member-htit clear-bor-b"><span class="member-titsc">常用邮寄地址维护</span></div>
					<div id="sendEmail">
						<div class="management_con cf">
							<div class="add_management">
								<span class="add_manage" id="addEmailAddress"></span>
								<div class="manage_mes">
										已有<b><s:property value="#request.deliverAddressCount" /></b>个常用邮寄地址，还可添加<b><s:property
												value="#request.leftdeliverAddressCount" /></b>个
								</div>
							</div>
							<ul id="sendEmail">						
								<s:iterator id="list" value="#request.deliverAddressList" step="1" status="st">
									<li>
										<p>
											<label class="label_radio" for="radio0<s:property value="#st.index+1" />">
												<s:if test="#request.isDefault==1">
													<input id="radio0<s:property value="#st.index+1" />" type="radio" name="radio" value="Y" checked>
												</s:if> 
												<s:else>
													<input id="radio0<s:property value="#st.index+1" />" type="radio" name="radio" value="Y">
												</s:else>
												<span class="cont"><s:property value="#list.provinceName" />&nbsp;<s:property value="#list.cityName" />&nbsp;<s:property value="#list.detailAddr" />
												<br /><s:property value="#list.name" />收&nbsp;<s:property value="#list.tel" />&nbsp;邮编：<s:property value="#list.zipCode" /></span>
											</label>
										</p>
										<p class="btns">
											<s:if test="#request.isDefault==1">
												默认&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
											</s:if>
											<s:else>
												<a class="defOn default" href="javascript:void(0);" onclick="setDefaultAddrinfo('<s:property value="#request.memberID" />', '<s:property value="#list.id" />');">设为默认</a>
											</s:else>
											<a class="require" href="javascript:void(0);" id="deliverAddrUpdate_<s:property value="#list.id" />">修改</a> 
											<a class="delete" href="javascript:void(0);" onclick="deleteDeliverAddrinfo('<s:property value="#list.id" />');">删除</a>
										</p>
									</li>
								</s:iterator>
							</ul> 
						</div>
					</div>
		 		</div>
		 		<!-- /常用信息维护 -->	
			</div>
		</div>
	</div>
	<!-- 底部开始 -->
	<s:include value="/wwwroot/kxb/block/kxb_footer_new_common.shtml"></s:include>
	<!-- 底部结束 -->
	
	<script type="text/javascript" src="<%=Config.getValue("JsResourcePath")%>/js/jquery-1.4.2.min.js"></script>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath")%>/js/jquery.cookie.js"></script>

<s:include value="/wwwroot/kxb/block/community_v1.shtml"></s:include>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath") %>/js/iframeTools.js"></script>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath") %>/js/template/shop/js/My97DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath") %>/js/template/shop/js/memberinfo.js"></script>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath") %>/js/new_member.js"></script>
<script type="text/javascript" src="<%=Config.getValue("JsResourcePath") %>/js/VerifyInput.js"></script>
